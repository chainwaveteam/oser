{
  "name": "oser-ecrire",
  "description": "The Oser-Ecrire Blog",
  "private": true,
  "version": "1.0.0",
  "license": "UNLICENSED",
  "author": "Julien <juliencaron@protonmail.com>",
  "scripts": {
    "build": "lerna run build --parallel",
    "dev": "lerna run dev --parallel",
    "format": "lerna run format",
    "build-studio": "lerna bootstrap && cd studio && yarn build",
    "build-web": "lerna bootstrap && (cd studio && GATSBY_SANITY_TOKEN=$GATSBY_SANITY_TOKEN npm run graphql-deploy) && (cd web && npm run build)",
    "graphql-deploy": "lerna run graphql-deploy",
    "lint": "lerna run lint",
    "postinstall": "lerna bootstrap",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@sanity/cli": "^1.149.9",
    "lerna": "^3.20.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Junscuzzy/oser-ecrire"
  },
  "bugs": {
    "url": "https://github.com/Junscuzzy/oser-ecrire/issues"
  }
}
