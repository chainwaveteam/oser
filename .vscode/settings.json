{
    "editor.formatOnSave": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "search.exclude": {
        "*/public/**": true,
        "*/dist/**": true,
        "*/.cache/**": true,
        "*/node_modules/**": true
    },
    "eslint.workingDirectories": [
		{
			"directory": "studio",
			"changeProcessCWD": true
        },
        {
			"directory": "web",
            "changeProcessCWD": true
        }
    ],
    "files.exclude": {
        "web/node_modules/wonka/esy.lock/**/*": false,
        "node_modules/npm-normalize-package-bin/*": true,
        "node_modules/npm-normalize-package-bin/.*": true,
        "node_modules/npm-normalize-package-bin/**/bin/": false,
        "node_modules/npm-normalize-package-bin/**/lib/": false,
        "node_modules/npm-normalize-package-bin/**/docs/": false,
        "node_modules/npm-normalize-package-bin/**/package.json": false,
        "node_modules/npm-normalize-package-bin/**/package-lock.json": false,
        "node_modules/npm-normalize-package-bin/**/README.md": false,
        "node_modules/npm-normalize-package-bin/**/CONTRIBUTING.md": false,
        "node_modules/npm-normalize-package-bin/**/LICENSE": false,
        "node_modules/npm-normalize-package-bin/**/CHANGELOG.md": false,
        "node_modules/npm-normalize-package-bin/**/example/": false,
        "node_modules/npm-normalize-package-bin/**/scripts/": false,
        "node_modules/npm-normalize-package-bin/**/tap-snapshots/": false,
        "node_modules/npm-normalize-package-bin/**/test/": false,
        "node_modules/npm-normalize-package-bin/**/.github/": false,
        "node_modules/npm-normalize-package-bin/**/.travis.yml": false,
        "node_modules/npm-normalize-package-bin/**/.gitignore": false,
        "node_modules/npm-normalize-package-bin/**/.gitattributes": false,
        "node_modules/npm-normalize-package-bin/**/coverage-map.js": false,
        "node_modules/npm-normalize-package-bin/**/map.js": false,
        "node_modules/npm-normalize-package-bin/**/index.js": false
    }
}